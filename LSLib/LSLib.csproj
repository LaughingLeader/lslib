<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net472</TargetFramework>
		<LangVersion>9.0</LangVersion>
		<OutputType>Library</OutputType>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>
	<ItemGroup>
		<Using Remove="System.IO" />
		<Using Remove="System.Net.Http" />
		<Using Include="System.Runtime.InteropServices" />
		<Using Include="System.Text" />
	</ItemGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
		<DefineConstants>TRACE;DEBUG;EXPORT_GPPG</DefineConstants>
		<PlatformTarget>x64</PlatformTarget>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
		<DefineConstants>EXPORT_GPPG</DefineConstants>
		<PlatformTarget>x64</PlatformTarget>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Editor Debug|AnyCPU'">
		<OutputPath>bin\Editor Debug\</OutputPath>
		<DefineConstants>TRACE;EXPORT_GPPG</DefineConstants>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Optimize>true</Optimize>
		<PlatformTarget>x86</PlatformTarget>
		<CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
	</PropertyGroup>
	<ItemGroup>
		<Reference Include="QUT.ShiftReduceParser">
			<HintPath>..\external\gppg\binaries\QUT.ShiftReduceParser.dll</HintPath>
		</Reference>
		<Reference Include="System.Transactions" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\LSLibNative\LSLibNative.vcxproj" />
		<ProjectReference Include="..\OpenTK\OpenTK.csproj" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="AlphaFS" Version="2.2.6" />
		<PackageReference Include="lz4net" Version="1.0.15.93" />
		<PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="5.0.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="System.Buffers" Version="4.5.1" />
		<PackageReference Include="System.Memory" Version="4.5.5" />
		<PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
		<PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="4.5.3" />
		<PackageReference Include="System.Threading.Tasks.Extensions" Version="4.5.4" />
		<PackageReference Include="zlib.net" Version="1.0.4.0" />
		<PackageReference Include="ZstdSharp.Port" Version="0.7.4" />
	</ItemGroup>
	<PropertyGroup />

	<Target Name="PreBuild" BeforeTargets="PreBuildEvent">
		<ItemGroup>
			<GpLex Include="$(SolutionDir)\external\gppg\binaries\GpLex"/>
			<Gppg Include="$(SolutionDir)\external\gppg\binaries\Gppg"/>
		</ItemGroup>
		<Exec Command="@(GpLex) /out:&quot;$(ProjectDir)\LS\Story\GoalParser\Goal.lex.cs&quot; &quot;$(ProjectDir)\LS\Story\GoalParser\Goal.lex&quot;"/>
		<Exec Command="@(Gppg) /out:&quot;$(ProjectDir)\LS\Story\GoalParser\Goal.yy.cs&quot; &quot;$(ProjectDir)\LS\Story\GoalParser\Goal.yy&quot;"/>
		<Exec Command="@(GpLex) /out:&quot;$(ProjectDir)\LS\Story\HeaderParser\StoryHeader.lex.cs&quot; &quot;$(ProjectDir)\LS\Story\HeaderParser\StoryHeader.lex&quot;"/>
		<Exec Command="@(Gppg) /out:&quot;$(ProjectDir)\LS\Story\HeaderParser\StoryHeader.yy.cs&quot; &quot;$(ProjectDir)\LS\Story\HeaderParser\StoryHeader.yy&quot;"/>
		<Exec Command="@(GpLex) /out:&quot;$(ProjectDir)\LS\Stats\Parser\Stat.lex.cs&quot; &quot;$(ProjectDir)\LS\Stats\Parser\Stat.lex&quot;"/>
		<Exec Command="@(Gppg) /out:&quot;$(ProjectDir)\LS\Stats\Parser\Stat.yy.cs&quot; &quot;$(ProjectDir)\LS\Stats\Parser\Stat.yy&quot;"/>
		<Exec Command="@(GpLex) /out:&quot;$(ProjectDir)\LS\Stats\Parser\StatProperty.lex.cs&quot; &quot;$(ProjectDir)\LS\Stats\Parser\StatProperty.lex&quot;"/>
		<Exec Command="@(Gppg) /out:&quot;$(ProjectDir)\LS\Stats\Parser\StatProperty.yy.cs&quot; &quot;$(ProjectDir)\LS\Stats\Parser\StatProperty.yy&quot;"/>
		<Exec Command="@(GpLex) /out:&quot;$(ProjectDir)\LS\Stats\Parser\StatLua.lex.cs&quot; &quot;$(ProjectDir)\LS\Stats\Parser\StatLua.lex&quot;"/>
		<Exec Command="@(Gppg) /out:&quot;$(ProjectDir)\LS\Stats\Parser\StatLua.yy.cs&quot; &quot;$(ProjectDir)\LS\Stats\Parser\StatLua.yy&quot;"/>
	</Target>
</Project>